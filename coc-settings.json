{
  "languageserver": {
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "haskell-ide-engine": {
      "command": "stack",
      "args": ["exec", "--", "hie"],
      "trace.server": "verbose",
      "rootPatterns": [".stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {}
    },
    "reason-language-server": {
      "command": "reason-language-server",
      "filetypes": ["ocaml", "reason"]
    },
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    },
    "elixir-ls": {
      "command": "~/apps/elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "kotlin": {
      "command": "~/apps/KotlinLanguageServer/gradlew",
      "args": ["server:run"],
      "filetypes": ["kotlin"]
    }
  },
  "prettier.singleQuote": true,
  "prettier.disableLanguages": [],
  "coc.source.ultisnips.priority": 10,
  "coc.source.buffer.priority": 5,
  "coc.source.word.priority": 1,
  "codeLens.enable": true,
  "html.autoClosingTags": true,
  "explorer.keyMappings.global": {
    "j": ["wait", "nodeNext"],
    "k": ["wait", "nodePrev"]
  },
  "explorer.git.showIgnored": true,
  "explorer.filename.colored.enable": true,
  "list.normalMappings": {
    "q": "do:exit"
  },
  "git.branchCharacter": ""
}
